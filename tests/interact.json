{
    "project_name": {
        "type": "string",
        "default": "python3-demo",
        "description": "Project name"
    },
    "use_cython": {
        "type": "boolean",
        "default": false,
        "description": "Use cython"
    },
    "code_hosting": {
        "type": "choice",
        "default": 1,
        "choice": [
            "github",
            "gitee",
            "gitlab"
        ],
        "description": "Code hosting",
        "when": "use_cython == true"
    }
}
